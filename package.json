    "release:finish": "branch=$(git rev-parse --abbrev-ref HEAD | cut -d'/' -f2); git pull origin release/${branch} && git fetch && git pull origin main && npm run standard-version -- --release-as minor && git flow release finish -Fpn ${branch} && git push --tags",
		"hotfix:finish": "branch=$(git rev-parse --abbrev-ref HEAD | cut -d'/' -f2); git pull origin hotfix/${branch} && git fetch && git pull origin main && npm run standard-version -- --release-as patch && git flow hotfix finish -Fpn ${branch} && git push --tags"
  },
